{
  "presets": [
    [
      "react-native-builder-bob/babel-preset",

      { "targets": { "node": "current" } }
    ],
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "extensions": [".js", ".jsx", ".ts", ".tsx"],
        "alias": {
          "@src": "./src"
        }
      },
      "react-native-reanimated/plugin"
    ]
  ]
}
